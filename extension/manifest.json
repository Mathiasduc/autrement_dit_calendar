{
  "manifest_version": 2,
  "name": "Autrement dit calendar",
  "version": "0.5",

  "description": "Une extension pour aider a l'uniformisation des titres d'evenements Google calendar",
  "icons": {
    "19": "icon/logo19.png",
    "24": "icon/logo24.png",
    "32": "icon/logo32.png",
    "48": "icon/logo48.png",
    "128": "icon/logo128.png" 
  },

  "browser_action": {
    "default_icon": {                    
     "19": "icon/logo19.png",
     "24": "icon/logo24.png",
     "32": "icon/logo32.png",
     "48": "icon/logo48.png"
   },
   "default_title": "Calendar Autrement dit",
   "default_popup": "view.html"
 },

  "author": "Sammy Sanchez, Arsene Ntibushitse, Mathias Ducancel",
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },

  "options_page": "options.html",
  "options_ui": {
   "chrome_style": true,
   "page": "options.html"
 },

 "permissions": [
  "tabs",
  "clipboardRead",
  "clipboardWrite",
  "storage"

]
// "web_accessible_resources": [] //pourrait en avoir besoin
}